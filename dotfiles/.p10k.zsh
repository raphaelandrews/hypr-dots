  # Directory color
  typeset -g POWERLEVEL9K_DIR_FOREGROUND=$peach

  # Context format when root: user@host.
  typeset -g POWERLEVEL9K_CONTEXT_ROOT_TEMPLATE="%F{$rosewater}%n%f%F{$overlay0}@%m%f"
  typeset -g POWERLEVEL9K_CONTEXT_TEMPLATE="%F{$overlay1}%n@%m%f"
  typeset -g POWERLEVEL9K_CONTEXT_{DEFAULT,SUDO}_CONTENT_EXPANSION=

