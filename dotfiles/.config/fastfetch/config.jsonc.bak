{
  "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
  "logo": {
    "source": "arch",
    "type": "small",
    "height": 12,
    "padding": {
      "top": 4,
      "left": 3
    },
    "color": {
      "1": "#8bd5ca",
      "2": "#8AADF4"
    }
  },
  "modules": [
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;91;96;120m┌──────────────────────Hardware──────────────────────┐"
    },
    {
      "type": "title",
      "key": " PC",
      "keyColor": "#8bd5ca"
    },
    {
      "type": "cpu",
      "key": "│ ├",
      "showPeCoreCount": true,
      "keyColor": "#8bd5ca"
    },
    {
      "type": "gpu",
      "key": "│ ├",
      "detectionMethod": "pci",
      "keyColor": "#8bd5ca"
    },
    {
      "type": "display",
      "key": "│ ├󱄄",
      "keyColor": "#8bd5ca"
    },
    {
      "type": "disk",
      "key": "│ ├󰋊",
      "keyColor": "#8bd5ca"
    },
    {
      "type": "memory",
      "key": "│ ├",
      "keyColor": "#8bd5ca"
    },
    {
      "type": "swap",
      "key": "└ └󰓡 ",
      "keyColor": "#8bd5ca"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;91;96;120m└────────────────────────────────────────────────────┘"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;91;96;120m┌──────────────────────Software──────────────────────┐"
    },
    {
      "type": "command",
      "key": "󰣇 OS",
      "keyColor": "#f5bde6",
      "text": "version=$(omarchy-version); echo \"Omarchy $version\""
    },
    {
      "type": "command",
      "key": "│ ├󰘬",
      "keyColor": "#f5bde6",
      "text": "branch=$(omarchy-version-branch); echo \"$branch\""
    },
    {
      "type": "kernel",
      "key": "│ ├",
      "keyColor": "#f5bde6"
    },
    {
      "type": "wm",
      "key": "│ ├",
      "keyColor": "#f5bde6"
    },
    {
      "type": "de",
      "key": " DE",
      "keyColor": "#f5bde6"
    },
    {
      "type": "terminal",
      "key": "│ ├",
      "keyColor": "#f5bde6"
    },
    {
      "type": "packages",
      "key": "│ ├󰏖",
      "keyColor": "#f5bde6"
    },
    {
      "type": "wmtheme",
      "key": "│ ├󰉼",
      "keyColor": "#f5bde6"
    },
    {
      "type": "command",
      "key": "│ ├󰸌",
      "keyColor": "#f5bde6",
      "text": "theme=$(omarchy-theme-current); echo -e \"$theme \\e[38;2;122;162;247m●\\e[38;2;138;173;244m●\\e[38;2;183;189;248m●\\e[38;2;125;196;228m●\\e[38;2;165;218;149m●\\e[38;2;238;212;159m●\""
    },
    {
      "type": "terminalfont",
      "key": "└ └",
      "keyColor": "#f5bde6"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;91;96;120m└────────────────────────────────────────────────────┘"
    },
    "break",
    {
      "type": "custom",
      "format": "\u001b[38;2;91;96;120m┌────────────────────Uptime / Age────────────────────┐"
    },
    {
      "type": "command",
      "key": "󱦟 OS Age",
      "keyColor": "#b7bdf8",
      "text": "birth_install=$(stat -c %W /); current=$(date +%s); time_progression=$((current - birth_install)); days_difference=$((time_progression / 86400)); echo $days_difference days"
    },
    {
      "type": "uptime",
      "key": "󱫐 Uptime",
      "keyColor": "#b7bdf8"
    },
    {
      "type": "custom",
      "format": "\u001b[38;2;91;96;120m└────────────────────────────────────────────────────┘"
    },
    "break"
  ]
}